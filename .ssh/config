# Harvesters via VPN

Host harv3-vpn
	Hostname h3.aft.tips
	User aft

Host harv3-vpn.robot01
	Hostname aft-robot01
	HostKeyAlias harv3.robot01
	User aft
	ProxyCommand ssh -W %h:%p harv3-vpn

Host harv3-vpn.robot02
	Hostname aft-robot02
	HostKeyAlias harv3.robot02
	User aft
	ProxyCommand ssh -W %h:%p harv3-vpn

