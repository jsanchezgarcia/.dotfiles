	Hostname gpu2.aft.tips
	User aft
	IdentityFile ~/.ssh/id_rsa

Host harvbot
	Hostname harvbot.advanced.farm
	User ubuntu
	IdentityFile ~/.ssh/id_rsa

# DEV SETUP

Host aftdev
	Hostname 192.168.13.148
	User aft
	IdentityFile ~/.ssh/id_rsa

Host aftwc1dev
       Hostname 192.168.1.41
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftdev

Host aftwc2dev
       Hostname 192.168.1.42
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftdev


# MAIN HARVESTER
Host master
	Hostname 192.168.1.2
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
	User aft
	IdentityFile ~/.ssh/id_rsa

Host master.bot1
       Hostname 192.168.1.41
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p master

Host master.bot2
       Hostname 192.168.1.42
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p master

Host master.bot3
       Hostname 192.168.1.43
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p master

Host master.bot4
       Hostname 192.168.1.44
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p master


