Host gpu
	Hostname gpu.aft.tips
	User aft
	IdentityFile ~/.ssh/id_rsa

# DEV SETUP

Host aftdev
	Hostname 192.168.13.148
	User aft
	IdentityFile ~/.ssh/id_rsa

Host aftwc1dev
       Hostname 192.168.1.41
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftdev

Host aftwc2dev
       Hostname 192.168.1.42
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftdev


# MAIN HARVESTER
Host aftmaster
	Hostname 192.168.1.2
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
	User aft
	IdentityFile ~/.ssh/id_rsa

Host aftwc1
       Hostname 192.168.1.41
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftmaster

Host aftwc2
       Hostname 192.168.1.42
       StrictHostKeyChecking no
       UserKnownHostsFile=/dev/null
       User aft
       IdentityFile ~/.ssh/id_rsa
       ProxyCommand ssh -W %h:%p aftmaster

